{
    "manifest_version": 3,
    "name": "Open Links in Tabs",
    "version": "2024.5.31",
    "author": "Managed Kaos",
    "homepage_url": "https://github.com/managedkaos/open-links-in-tabs",
    "description": "URLs entered in a form are opened in tabs.",
    "icons": {
        "16":  "img/folder-blue-16.png",
        "48":  "img/folder-blue-48.png",
        "128": "img/folder-blue-128.png"
    },
    "action": {
        "default_icon" : {
            "16" : "img/folder-blue-16.png",
            "38" : "img/folder-blue-48.png"
        },
        "default_title": "Open Links in Tabs",
        "default_popup": "popup.html"
    }
}
